<?xml version='1.0' encoding='UTF-8'?>

<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN"
  "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">

<module rename-to='structtree'>

	<inherits name='com.google.gwt.user.User' />

	<inherits name='ru.ot.mvp.GWT-MVP' />
	<inherits name='com.intendia.rxgwt2.RxUser' />
	<inherits name='ru.ot.gwt.utils.GWT-Utils' />
	<inherits name='ru.ot.gwt.sdo.GWT-SDO' />
	<inherits name='ru.ot.wevelns.WeVEL-NS' />
	<inherits name='ru.ot.ot_132_5_0030.OT_132_5_0030' />
	<inherits name="ru.ot.gwt.exception.GWT-Exception" />

	<set-configuration-property
		name='xsiframe.failIfScriptTag' value='FALSE' />

	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<inherits name="com.google.gwt.i18n.I18N" />
	<entry-point class='com.otoil.client.StructTree' />
	<extend-property name="locale" values="ru" /> 

	<source path='client' />
	<source path='shared' />

	<add-linker name="xsiframe" />
</module>
